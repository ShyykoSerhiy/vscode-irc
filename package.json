{
  "name": "vscode-irc",
  "description": "An IRC Client in VSCode",
  "version": "0.0.1",
  "publisher": "audstanley",
  "engines": {
    "vscode": "^1.23.0"
  },
  "author": "Richard Stanley",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:irc.start",
    "onCommand:irc.doRefactor"
  ],
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "irc.start",
        "title": "Start connection to IRC",
        "category": "IRC"
      },
      {
        "command": "irc.doRefactor",
        "title": "Do some refactoring",
        "category": "IRC"
      }
    ]
  },
  "dependencies": {
    "irc-socket": "^3.0.8",
    "net": "^1.0.2",
    "node-fetch": "^2.1.2",
    "vscode": "^1.1.18"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "typescript": "^2.7.2"
  }
}
